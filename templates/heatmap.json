{
  "__inputs": [ ],
  "__requires": [ ],
  "annotations": {
    "list": [ ]
  },
  "editable": false,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
{% for panel in item.panels %}
    {
{% if (panel.type is not defined) or (panel.type == "heatmap") %}
      "cards": {
        "cardPadding": null,
        "cardRound": null
      },
      "color": {
        "cardColor": "{{ panel.card_color }}",
        "colorScale": "sqrt",
        "colorScheme": "interpolateSpectral",
        "exponent": 0.3,
        "mode": "opacity"
      },
      "dataFormat": "tsbuckets",
{% endif %}
{% if panel.value_name is defined %}
      "valueName": "{{ panel.value_name }}",
{% endif %}
{% if panel.color_value is defined %}
      "colorValue": {{ panel.color_value }},
{% endif %}
      "gridPos": {
        "h": {% if panel.gridpos_h is defined %}{{ panel.gridpos_h }}{% else %}14{% endif %},
        "w": {% if panel.gridpos_w is defined %}{{ panel.gridpos_w }}{% else %}24{% endif %},
        "x": {% if panel.gridpos_x is defined %}{{ panel.gridpos_x }}{% else %}0{% endif %},
        "y": {% if panel.gridpos_y is defined %}{{ panel.gridpos_y }}{% else %}0{% endif %}
      },
      "heatmap": {},
      "highlightCards": true,
      "id": {{ 1 + loop.index }},
      "legend": {
        "show": false
      },
      "links": [],
      "targets": [
{% for target in panel.targets %}
        {
          "expr": "{{ target.expr }}",
          "format": "{% if target.format is defined %}{{ target.format }}{% else %}heatmap{% endif %}",
          "intervalFactor": {% if target.interval_factor is defined %}{{ target.interval_factor }}{% else %}1{% endif %},
          "legendFormat": "{{ target.legend_format }}",
          "refId": "A"
        }{% if not loop.last %},{% endif %}
{% endfor %}
      ],
      "title": "{{ panel.name }}",
      "tooltip": {
        "show": true,
        "showHistogram": false
      },
      "type": "{% if panel.type is defined %}{{ panel.type }}{% else %}heatmap{% endif %}",
      "xAxis": {
        "show": true
      },
      "xBucketNumber": null,
      "xBucketSize": null,
{% if (panel.type is not defined) or (panel.type != "singlestat") %}
      "yAxis": {
        "decimals": {{ panel.decimals }},
        "format": "{{ panel.format }}",
        "logBase": 1,
        "max": null,
        "min": null,
        "show": true,
        "splitFactor": null
      },
{% endif %}
      "yBucketBound": "auto",
      "yBucketNumber": null,
      "yBucketSize": null
    }{% if not loop.last %},{% endif %}
{% endfor %}
  ],
  "refresh": "{% if item.refresh is defined %}{{ item.refresh }}{% else %}30s{% endif %}",
  "schemaVersion": 16,
  "style": "dark",
  "tags": [
{% for tag in item.tags %}
    "{{ tag }}"{% if not loop.last %},{% endif %}
{% endfor %}
  ],
  "templating": {
    "list": [
{% for template in item.templates %}
      {
        "allValue": null,
        "current": { },
        "datasource": "default",
        "hide": 0,
        "includeAll": {{ template.includeAll }},
        "label": "{{ template.label }}",
        "multi": false,
        "name": "{{ template.name }}",
        "options": [ ],
        "query": "{{ template.query }}",
        "refresh": 2,
        "regex": "",
        "sort": 0,
        "tagValuesQuery": "",
        "tags": [ ],
        "tagsQuery": "",
        "type": "query",
        "useTags": false
      }{% if not loop.last %},{% endif %}
{% endfor %}
    ]
  },
  "time": {
    "from": "{{ item.time_from }}",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ],
    "time_options": [
      "5m",
      "15m",
      "1h",
      "6h",
      "12h",
      "24h",
      "2d",
      "7d",
      "30d"
    ]
  },
  "timezone": "browser",
  "title": "{{ item.dashboard_name }}",
  "version": 0
}
